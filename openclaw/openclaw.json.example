{
  "agent": {
    "workspace": "~/clawd",
    "model": {
      "primary": "openai-compatible/your-model-id",
      "fallbacks": ["openai-compatible/backup-model-id"]
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "openai-compatible": {
        "baseUrl": "https://your-api.com/v1",
        "apiKey": "${CUSTOM_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "your-model-id",
            "name": "Your Model Name",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": {"input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0},
            "contextWindow": 128000,
            "maxTokens": 4096
          },
          {
            "id": "backup-model-id",
            "name": "Backup Model",
            "reasoning": false,
            "input": ["text"],
            "cost": {"input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0},
            "contextWindow": 32000,
            "maxTokens": 2048
          }
        ]
      },
      "nvidia": {
        "baseUrl": "https://integrate.api.nvidia.com/v1",
        "apiKey": "${NVIDIA_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "minimaxai/minimax-m2.1",
            "name": "MiniMax M2.1",
            "reasoning": true,
            "input": ["text", "image"],
            "cost": {"input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0},
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      },
      "openrouter": {
        "baseUrl": "https://openrouter.ai/api/v1",
        "apiKey": "${OPENROUTER_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "anthropic/claude-3.5-sonnet",
            "name": "Claude 3.5 Sonnet",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": {"input": 3, "output": 15, "cacheRead": 0.3, "cacheWrite": 3.75},
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "openai/gpt-4o",
            "name": "GPT-4o",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": {"input": 2.5, "output": 10, "cacheRead": 1.25, "cacheWrite": 3.75},
            "contextWindow": 128000,
            "maxTokens": 16384
          }
        ]
      },
      "deepseek": {
        "baseUrl": "https://api.deepseek.com/v1",
        "apiKey": "${DEEPSEEK_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "deepseek-chat",
            "name": "DeepSeek Chat",
            "reasoning": false,
            "input": ["text"],
            "cost": {"input": 0.14, "output": 0.28, "cacheRead": 0.014, "cacheWrite": 0.14},
            "contextWindow": 64000,
            "maxTokens": 8192
          },
          {
            "id": "deepseek-reasoner",
            "name": "DeepSeek Reasoner",
            "reasoning": true,
            "input": ["text"],
            "cost": {"input": 0.55, "output": 2.19, "cacheRead": 0.014, "cacheWrite": 0.14},
            "contextWindow": 64000,
            "maxTokens": 8192
          }
        ]
      },
      "ollama": {
        "baseUrl": "http://127.0.0.1:11434/v1",
        "apiKey": "ollama",
        "api": "openai-completions",
        "models": [
          {
            "id": "qwen2.5-coder:32b",
            "name": "Qwen2.5 Coder 32B",
            "reasoning": false,
            "input": ["text"],
            "cost": {"input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0},
            "contextWindow": 32768,
            "maxTokens": 8192
          },
          {
            "id": "deepseek-r1:32b",
            "name": "DeepSeek R1 32B",
            "reasoning": true,
            "input": ["text"],
            "cost": {"input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0},
            "contextWindow": 65536,
            "maxTokens": 8192
          },
          {
            "id": "llama3.3:70b",
            "name": "Llama 3.3 70B",
            "reasoning": false,
            "input": ["text"],
            "cost": {"input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0},
            "contextWindow": 128000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "channels": {
    "whatsapp": {
      "allowFrom": ["*"],
      "dmPolicy": "open",
      "groups": {
        "*": {
          "requireMention": true
        }
      }
    },
    "telegram": {
      "enabled": false,
      "botToken": "YOUR_TELEGRAM_BOT_TOKEN",
      "allowFrom": ["123456789", "987654321"],
      "groupPolicy": "allowlist",
      "groupAllowFrom": ["123456789"],
      "groups": {
        "*": {
          "requireMention": true
        }
      }
    },
    "discord": {
      "enabled": false,
      "token": "YOUR_DISCORD_BOT_TOKEN",
      "dm": {
        "enabled": true,
        "allowFrom": ["username#1234", "another_user"]
      },
      "guilds": {
        "123456789012345678": {
          "slug": "my-server",
          "requireMention": false,
          "channels": {
            "general": {
              "allow": true
            },
            "bot-commands": {
              "allow": true,
              "requireMention": false
            }
          }
        }
      }
    },
    "slack": {
      "enabled": false,
      "botToken": "YOUR_SLACK_BOT_TOKEN",
      "appToken": "YOUR_SLACK_APP_TOKEN",
      "dm": {
        "enabled": true,
        "allowFrom": ["U01234567", "U98765432"]
      },
      "channels": {
        "#general": {
          "allow": true,
          "requireMention": true
        },
        "#bot-testing": {
          "allow": true,
          "requireMention": false
        }
      },
      "slashCommand": {
        "enabled": true,
        "name": "bot",
        "sessionPrefix": "slack:slash",
        "ephemeral": true
      }
    },
    "signal": {
      "enabled": false,
      "phoneNumber": "+1234567890",
      "allowFrom": ["+9876543210"],
      "groups": {
        "*": {
          "requireMention": true
        }
      }
    },
    "imessage": {
      "enabled": false,
      "allowFrom": ["user@example.com", "+1234567890"],
      "groups": {
        "*": {
          "requireMention": true
        }
      }
    },
    "line": {
      "enabled": false,
      "channelSecret": "your-channel-secret",
      "channelAccessToken": "your-channel-access-token",
      "allowFrom": ["U1234567890abcdef1234567890abcdef"]
    },
    "googlechat": {
      "enabled": false,
      "credentialsPath": "~/credentials.json",
      "spaces": {
        "spaces/AAAA1234567": {
          "allow": true,
          "requireMention": true
        }
      }
    },
    "msteams": {
      "enabled": false,
      "appId": "12345678-1234-1234-1234-123456789012",
      "appPassword": "your-app-password",
      "allowFrom": ["user@company.com"]
    }
  },
  "identity": {
    "name": "Bot",
    "theme": "helpful assistant",
    "emoji": "ðŸ¤–"
  },
  "session": {
    "scope": "per-sender",
    "reset": {
      "mode": "daily",
      "atHour": 4
    }
  }
}
